<!-- Contact Section -->
<section id="contact">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading">Contact</h2>
                <h3 class="section-subheading text-muted">Une question? Un projet en tête? Besoin d'un devis? Laissez-nous un petit message.</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
<!--                 <form name="sentMessage" id="contactForm" novalidate>
 -->
                    <%= simple_form_for(@new_message) do |f| %>

                    <div class="row">
                        <div class="col-md-6">
                          <div class="row">
                            <div class="col-md-6">
                                <%= f.input :name, :placeholder => "Votre nom", label: false %>
                            </div>
                            <div class="col-md-6">
                                <%= f.input :first_name, :placeholder => "Et prénom", label: false %>
                            </div>
                          </div>
                            <%= f.input :email, :placeholder => "Votre email", label: false %>
                            <%= f.input :phone, :placeholder => "Votre téléphone", label: false %>
                        </div>
                        <div class="col-md-6">
                            <%= f.input :subject, :placeholder => "Sujet du message", label: false %>
                            <%= f.input :description, :placeholder => "Votre message", label: false %>                            
                        </div>
                        <div class="clearfix"></div>
                        <div class="col-lg-12 text-center">
                            <div id="success"></div>
                            <%= f.submit 'Envoyer', class: "btn btn-xl" %>
                        </div>
                    </div>
                  <% end %>

                <!-- </form> -->
            </div>
        </div>
    </div>
</section>

