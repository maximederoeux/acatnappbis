<!-- Contact Section -->
<section id="contact">
  <div class="container">
      <div class="row">
          <div class="col-lg-12 text-center">
              <h2 class="section-heading"><%= t("contact.contact") %></h2>
              <h3 class="section-subheading text-muted"><%= t("contact.an_idea") %></h3>
          </div>
      </div>
      <div class="row">
          <div class="col-lg-12">
<!--                 <form name="sentMessage" id="contactForm" novalidate>
-->
                  <%= simple_form_for(@new_message) do |f| %>

                  <div class="row">
                      <div class="col-md-6">
                        <div class="row">
                          <div class="col-md-6">
                              <%= f.input :name, :placeholder => t("contact.name"), label: false %>
                          </div>
                          <div class="col-md-6">
                              <%= f.input :first_name, :placeholder => t("contact.first_name"), label: false %>
                          </div>
                        </div>
                          <%= f.input :email, :placeholder => t("contact.email"), label: false %>
                          <%= f.input :phone, :placeholder => t("contact.phone"), label: false %>
                      </div>
                      <div class="col-md-6">
                          <%= f.input :subject, :placeholder => t("contact.subject"), label: false %>
                          <%= f.input :description, :placeholder => t("contact.content"), label: false %>                            
                      </div>
                      <div class="clearfix"></div>
                      <div class="col-lg-12 text-center">
                          <div id="success"></div>
                          <%= f.submit t("contact.send"), class: "btn btn-xl" %>
                      </div>
                  </div>
                <% end %>

              <!-- </form> -->
          </div>
      </div>
  </div>
</section>

<% if user_signed_in? %>

  <section id="messages">
    <div class="container">
      <ul class="list-group">

          <% @messages.each do |message| %>

        <li class="list-group-item list-group-item-warning">
          <div class="row">
            <div class="col-md-2">
             <%= l(message.created_at, :format => "%d/%m/%Y") %>
            </div>
            <div class="col-md-4">
              <b><%= message.name %></b>
            </div>
            <div class="col-md-5">
              <%= message.subject %>
            </div>
            <div class="col-md-1">
              <%= message.read %>
            </div>
          </div>
        </li>



        <li class="list-group-item">

          <%= message.description %>

        </li>

        <% end %>


      </ul>

    </div>
  </section>

<% end %>

